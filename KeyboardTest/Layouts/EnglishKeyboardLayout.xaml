				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="2" Grid.Row="0"  Content="{Binding Position_3_2Display}"	 CommandParameter="3|2" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="3" Grid.Row="0"  Content="{Binding Position_3_3Display}"	 CommandParameter="3|3" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="4" Grid.Row="0"  Content="{Binding Position_3_4Display}"	 CommandParameter="3|4" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="5" Grid.Row="0"  Content="{Binding Position_3_5Display}"	 CommandParameter="3|5" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="6" Grid.Row="0"  Content="{Binding Position_3_6Display}"	 CommandParameter="3|6" />
