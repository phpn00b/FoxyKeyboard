				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="9" Grid.Row="0"  Content="{Binding Position_1_9Display}"	 CommandParameter="1|9"/>
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="10" Grid.Row="0" Content="{Binding Position_1_10Display}" CommandParameter="1|10" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="11" Grid.Row="0" Content="{Binding Position_1_11Display}" CommandParameter="1|11" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="12" Grid.Row="0" Content="{Binding Position_1_12Display}" CommandParameter="1|12" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="13" Grid.Row="0" Content="{Binding Position_1_13Display}" CommandParameter="1|13" />
