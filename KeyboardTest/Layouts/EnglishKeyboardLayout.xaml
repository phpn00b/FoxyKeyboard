				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="5" Grid.Row="0"  Content="{Binding Position_0_5Display}"  CommandParameter="0|5" />
