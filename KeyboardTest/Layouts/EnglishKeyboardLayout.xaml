				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="7" Grid.Row="0"  Content="{Binding Position_0_7Display}"  CommandParameter="0|7" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="8" Grid.Row="0"  Content="{Binding Position_0_8Display}"  CommandParameter="0|8" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="9" Grid.Row="0"  Content="{Binding Position_0_9Display}"  CommandParameter="0|9" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="10" Grid.Row="0" Content="{Binding Position_0_10Display}" CommandParameter="0|10" />
