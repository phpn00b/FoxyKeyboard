			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition Height="0.20*" MinHeight="50" MaxHeight="50"/>
				<RowDefinition Height="0.2*"/>
				<RowDefinition Height="0.2*"/>
				<RowDefinition Height="0.2*"/>
				<RowDefinition Height="0.2*"/>
				<RowDefinition Height="0.2*"/>
			</Grid.RowDefinitions>


			<Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" MinHeight="30" >
