			<Grid Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="1" Visibility="{Binding FullKeyboardVisibility}">
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="0.166666667*" />
					<ColumnDefinition Width="0.066666667*" />
