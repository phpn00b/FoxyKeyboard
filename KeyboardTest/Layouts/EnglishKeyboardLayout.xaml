				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="3"  Grid.Row="0" Content="{Binding Position_2_3Display}"	 CommandParameter="2|3" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="4"  Grid.Row="0" Content="{Binding Position_2_4Display}"	 CommandParameter="2|4" />
				<Button Command="{Binding KeyPressCommand}" Style="{StaticResource {ComponentResourceKey {x:Type local:EnglishKeyboardLayout}, KeyboardButton}}" Grid.Column="5"  Grid.Row="0" Content="{Binding Position_2_5Display}"	 CommandParameter="2|5" />
